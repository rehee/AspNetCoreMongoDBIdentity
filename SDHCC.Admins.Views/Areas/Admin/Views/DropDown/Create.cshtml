@model DropDownPostModel
@{
    ViewData["Title"] = "Content";
    Layout = "_Layout_Content";
    var contentId = Model != null ? Model.Id : "";
}
@section breadcrumb{
    @{ await Html.RenderPartialAsync("_BreadCrumbDropDown", new string[] { ViewBag.name ,"Create" });}
}
<div class="card">
    <div class="card-block">
        <h4>Default Basic Forms</h4>
        <form action="@Url.Action("Create","DropDown",new { @area="Admin" })" class="form-horizontal m-t-40" method="post" enctype="multipart/form-data">
            @{ await Html.RenderPartialAsync("_CreateOrEdit", Model);}
        </form>

    </div>

</div>
